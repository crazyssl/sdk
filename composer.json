{
    "name": "crazyssl/sdk",
    "description": "The sdk for CrazySSL",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "xiaohui.lam",
            "email": "xiaohui.lam@aliyun.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": ">=5.0"
    },
    "require-dev": {
        "laravel/framework": ">=5.0",
        "laravel/laravel": ">=5.0",
        "squizlabs/php_codesniffer": "3.*",
        "phpunit/phpunit": "<8.0",
        "xiaohuilam/laravel-test": ">=v0.0.1"
    },
    "autoload": {
        "psr-4": {
            "Crazyssl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Crazyssl\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "laravel/framework": "在 Laravel 5.5+ 框架上使用, 效果更佳"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Crazyssl\\LaravelServiceProvider"
            ]
        }
    }
}
